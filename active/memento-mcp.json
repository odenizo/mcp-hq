{
  "metadata": {
    "server_id": "memento",
    "display_name": "Memento MCP Server",
    "namespace": "local",
    "package": "@gannonh/memento-mcp",
    "version": "1.0.0",
    "description": "MCP server for Memento, a persistent knowledge graph memory system",
    "use_cases": [
      "Store and retrieve information in a knowledge graph",
      "Perform semantic searches on stored data"
    ],
    "tools": [],
    "resources": [],
    "requirements": {
      "authentication": "none",
      "api_keys_needed": [],
      "external_dependencies": ["Neo4j"],
      "network_access": true,
      "file_system_access": false,
      "database_access": true
    },
    "performance": {
      "response_time": "fast",
      "resource_usage": "medium",
      "concurrent_requests": 10,
      "rate_limits": ""
    },
    "maturity": {
      "stability": "stable",
      "maintenance": "active",
      "community": "medium",
      "documentation": "good"
    },
    "planned_agent": {
      "assigned": false
    },
    "priority_deployment": "local",
    "tags": ["memento", "knowledge-graph", "memory"]
  },
  "deployment": {
    "local": {
      "available": true,
      "command": "npx",
      "args": [
        "-y",
        "@gannonh/memento-mcp"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "*"
      ]
    }
  }
}